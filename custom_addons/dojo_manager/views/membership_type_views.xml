<odoo>
    <data>
        <record id="view_membership_type_tree" model="ir.ui.view">
            <field name="name">dojo.membership.type.tree</field>
            <field name="model">dojo.membership.type</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name" />
                    <field name="default_price" />
                    <field name="default_duration" />
                </tree>
            </field>
        </record>

        <record id="action_membership_type" model="ir.actions.act_window">
            <field name="name">Membership Plans</field>
            <field name="res_model">dojo.membership.type</field>
            <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>